&RELEASES_CTRL
 NSPEC      =           1, ! Total number of species
 SPECNUM_REL=          24, ! Species numbers in directory SPECIES
 /
&RELEASE                   ! For each release 
 IDATE1  = 20010909, ! Release start date, YYYYMMDD: YYYY=year, MM=month, DD=day
 ITIME1  = 000000, ! Release start time in UTC HHMISS: HH hours, MI=minutes, SS=seconds
 IDATE2  = 20010909, ! Release end date, same as IDATE1
 ITIME2  = 000000, ! Release end time, same as ITIME1
 LON1    = 1, ! Left longitude of release box -180 < LON1 <180
 LON2    = 3, ! Right longitude of release box, same as LON1
 LAT1    = 2, ! Lower latitude of release box, -90 < LAT1 < 90
 LAT2    = 4, ! Upper latitude of release box same format as LAT1 
 Z1      = 0, ! Lower height of release box meters/hPa above reference level
 Z2      = 100, ! Upper height of release box meters/hPa above reference level
 ZKIND   = 1, ! Reference level 1=above ground, 2=above sea level, 3 for pressure in hPa
 MASS    = 1, ! Total mass emitted, only relevant for fwd simulations
 PARTS   = 100, ! Total number of particles to be released
 COMMENT = lalal, ! Comment, written in the outputfile
 /
